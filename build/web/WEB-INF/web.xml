<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servletDispatcher</servlet-name>
        <servlet-class>Cotrolador.servletDispatcher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletLogin</servlet-name>
        <servlet-class>Cotrolador.servletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletLogout</servlet-name>
        <servlet-class>Cotrolador.servletLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletVotar</servlet-name>
        <servlet-class>Cotrolador.servletVotar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletVoto</servlet-name>
        <servlet-class>Cotrolador.servletVoto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletEscrutinio</servlet-name>
        <servlet-class>Cotrolador.servletEscrutinio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletCenso</servlet-name>
        <servlet-class>Cotrolador.servletCenso</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletDispatcher</servlet-name>
        <url-pattern>/servletDispatcher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletLogin</servlet-name>
        <url-pattern>/servletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletLogout</servlet-name>
        <url-pattern>/servletLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletVotar</servlet-name>
        <url-pattern>/servletVotar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletVoto</servlet-name>
        <url-pattern>/servletVoto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletEscrutinio</servlet-name>
        <url-pattern>/servletEscrutinio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletCenso</servlet-name>
        <url-pattern>/servletCenso</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
